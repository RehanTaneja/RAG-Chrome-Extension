{
  "manifest_version": 3,
  "name": "PDF Text Extractor",
  "version": "1.0",
  "permissions": ["scripting", "activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["<all_urls>"]
}
