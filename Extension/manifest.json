{
  "manifest_version": 3,
  "name": "PDF Clipboard Listener",
  "version": "1.0",
  "description": "Detects copied text and shows it in the popup.",
  "permissions": [
    "clipboardRead",
    "storage",
    "tabs"
  ],
  "host_permissions": [
  "https://rag-service-868962904696.us-central1.run.app/*",
  "<all_urls>"            
],
  "action": {
    "default_popup": "popup.html"
  }
}
